    "def g(n):\n",
    "    if n==1:\n",
    "        return [\"0\", \"1\"]\n",
    "    else:\n",
    "        L = g(n-1)\n",
    "        left = [\"0\"+L[i] for i in range(len(L))]\n",
    "        right = [\"1\"+L[-1-i] for i in range(len(L))]\n",
    "        return left+right"
   ]
  },
